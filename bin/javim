#!/usr/bin/env ruby
unless respond_to? :require_relative
  def require_relative(o)
    require File.dirname(__FILE__) + "/#{o}"
  end
end
require_relative '../lib/javim'

jarfile = '~/Dropbox/hoot/SOFTBANK_MEXA_EMULATOR23/lib/stubclasses.zip'
table = Javim.global.merge Javim.local(jarfile)
keywords = Javim.keywords_of ARGV.shift
puts keywords.map {|k|
  (table[k] || []).map {|package|
    "import #{package};"
  }
}.flatten
